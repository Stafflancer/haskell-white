	"styles": {
		"color": {
			"background": "#fff",
			"text": "#333333"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--secondary)",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--background)",
						"text": "var(--wp--preset--color--foreground)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--background)",
						"text": "var(--wp--preset--color--foreground)"
					}
				},
				":active": {
					"border": {
						"color": "var(--wp--preset--color--foreground)"
					}
				},
				":visited": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--foreground)"
					}
				},
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			
			"heading": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--quaternary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--source-sans-pro)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)"
		},
		"blocks": {
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--secondary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lora)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"fontStyle": "italic",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)"
				}
			}
		}
	}
