{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/hero-banner-common",
	"title": "Hero Banner - Common",
	"description": "A custom hero banner common block.",
	"category": "acf",
	"icon": "admin-home",
	"keywords": [
		"hero",
		"banner",
		"home"
	],
	"acf": {
		"mode": "preview",
		"renderTemplate": "hero-banner-common.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		},
		"textColor": {
			"type": "string",
			"default": "foreground"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"fullHeight": true,
		"html": false,
		"jsx": true,
		"multiple": false,
		"reusable": false
	},
	"style": "file:./hero-banner-common.css",
	"example": {
		"attributes": {
			"mode": "preview",
			"data": {
				"_is_preview": "true"
			}
		}
	}
}
