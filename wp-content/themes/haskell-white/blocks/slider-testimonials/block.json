{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/slider-testimonials",
	"title": "Slider - Testimonials",
	"description": "A custom Slider - Testimonials block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "testimonial",
	"keywords": [ "slider", "testimonials" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "slider-testimonials.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center top"
		},
		"textColor": {
			"type": "string",
			"default": "foreground"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"fullHeight": false,
		"html": false,
		"jsx": true,
		"multiple": true,
		"reusable": false
	},
	"example": {
		"attributes": {
			"mode": "preview",
			"data": {
				"_is_preview": "true"
			}
		}
	},
	"style": [ "swiperjs-style", "file:./slider-testimonials.css"],
	"script": [ "swiperjs-script", "slider-testimonials-script"]
}